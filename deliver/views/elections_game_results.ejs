            /*
             $.getJSON("http://oknesset.org/api/member/" +open_knesset_id[i]+ "?callback=?", null, function(data) {

             });
             */


            $.getJSON("http://oknesset.org/api/v2/member-agendas/" + open_knesset_id[i] + "/?callback=?", null,function(data) {
                $.each(data.agendas, function (index, agenda) {
                    if(agenda.id==74) {
                        agenda.link=open_knesset_link[i];
                        dust.render('elections_game_social_index', agenda, function (err, out) {
                            $('#social_index'+i).append(out);
                        });
                    }
                });
            });


            $.getJSON("http://www.sandtalks.co.il/api/ranks?consumerKey=jdjhjdewjgdoc1rjwfdjcmzvn1fatlivduxcd09vnmivthpmate1uutey3dlmkmwbvpisc4zshuvsm9p&EntityId="+ sandtalk_id[i]+ "&callback=?", null,function(data) {
                data.name=name[i];
                data.margin=50;
                data.bottom_bar_color =data.beleavePercent <=33 ? "red":(data.beleavePercent <=66 ? "yellow":"green") ;
                data.top_bar_color  =data.agreePercent <=33 ? "red":(data.agreePercent <=66 ? "yellow":"green") ;
                dust.render('elections_game_sandtalk', data, function (err, out) {
                    $('#sandtalk'+i).prepend(out);
                    $('.kahol').show();
                });
            });

            $.getJSON("http://www.sandtalks.co.il/api/ranks?consumerKey=jdjhjdewjgdoc1rjwfdjcmzvn1fatlivduxcd09vnmivthpmate1uutey3dlmkmwbvpisc4zshuvsm9p&EntityId="+ prev_party_sandtalk_id[i]+ "&callback=?", null,function(data) {
                data.name=prev_party_name[i];
                data.margin=0;
                data.bottom_bar_color =data.beleavePercent <=33 ? "red":(data.beleavePercent <=66 ? "yellow":"green") ;
                data.top_bar_color  =data.agreePercent <=33 ? "red":(data.agreePercent <=66 ? "yellow":"green") ;
                dust.render('elections_game_sandtalk', data, function (err, out) {
                    $('#sandtalk'+i).append(out);
                    $('.kahol').show();

                });
            });


        }

