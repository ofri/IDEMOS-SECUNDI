        function getCandidateQuotes(id)   {

            db_functions.getQuoteGameQuotes(false,id,function (err, data) {
                var i=0;
                $('.quotes_holder').html("");
                $.each(data.objects, function (index, quote) {

                    quote.odd= i%2 ? 'odd' : 'even';
                    if(i>2) quote.odd+=' hidden_quotes'
                    quote.vote= quote.voted?  quote.voted.response:'';
                    dust.render('elections_game_additiona_quote', quote, function (err, out) {
                        $('.quotes_holder').append(out);
                    });
                    i++;

                });
                $('#more_quotes').show();

            }) ;
        }


        $('#more_quotes').click(function( ){
            $('.hidden_quotes').show() ;
            $('#more_quotes').hide();

        } );


        function loadAgenda(ind,agenda){
            $('#openk_'+ind).attr('src','http://ydaniv.github.com/ok-agenda-viz/?agenda_id='+agenda);
           // $('#openk_'+ind).attr('src','http://ydaniv.github.com/ok-agenda-viz/?agenda_id='+agenda+'#party_'+prev_party_open_knesset_id[ind]);
        }
        function getCandidateData(i){
             if(data_loaded[i]){
                 return;
             }else
             {
                 data_loaded[i]=true;
