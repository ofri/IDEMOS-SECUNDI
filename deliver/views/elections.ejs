<!DOCTYPE html>

<html>
<head>
    <%- partial('partials/head.ejs') %>
    <script src="/resources/js/jquery-fieldselection.js" type="text/javascript"></script>
</head>
<body>
<%- partial('partials/body_top.ejs') %>
<div id="wrap">
    <div id="header">
        <%-partial('partials/menu.ejs')%>
        <%-partial('partials/user_box.ejs')%>
        <%-partial('partials/failures.ejs')%>
        <%-partial('partials/tag_search.ejs') %>
    </div>

    <div id="content" class="cf">
        <h2><a href="javascript:void(0);" style="text-decoration: none;">
            עורו - הבית של הרוב
        </a></h2>
        <div class="rightCol">
            <div class="main-text">

            </div>
        </div>

        <div class="leftCol">
            <div class="about-left">

            </div>
        </div>
    </div>

</div>
</div>


<%- partial('partials/footer.ejs') %>


<script type="text/javascript">

    $(document).ready(function () {
        db_functions.getElectionsTexts(function (data) {
            $.each(data.objects, function (index, item) {
                dust.render('elections_text', item, function (err, out) {
                    $('.main-text').append(out);
                });
            });

        });

        db_functions.getElectionsItems(function (data) {

            $.each(data.objects, function (index, item) {
                dust.render('elections_item', item, function (err, out) {
                    $('.about-left').append(out);
                });
            });
        });
    });

</script>


</body>
</html>